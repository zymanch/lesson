{
  "name": "zymanch/lesson",
  "description": "Lesson",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "2.0.35",
    "yiisoft/yii2-bootstrap": "^2.0",
    "tightenco/collect": "5.4.33",
    "gtflixtv/active-generator": "dev-master",
    "yiisoft/yii2-debug": "^2.1",
    "spipu/html2pdf": "dev-master",
    "yiisoft/yii2-gii": "^2.2",
    "yiisoft/yii2-faker": "^2.0"
  },
  "autoload": {
    "psr-0": {
      "": "src/",
      "tests": "tests/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enable": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "ssh://git@devbase.gtflixtv.com/active-record.git"
    },
    {
      "type": "git",
      "url": "ssh://git@devbase.gtflixtv.com/active-generator.git"
    }
  ]
}
